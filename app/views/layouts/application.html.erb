<!DOCTYPE html>
<html>
  <head>
    <title>NotAllTrails</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <script type="text/javascript">
      window.linkedin = "<%= image_url('linkedin.png') %>"
      window.github = "<%= image_url('github.png') %>"
      window.logo = "<%= image_url('logo.png') %>"
      window.icon = "<%= image_url('profile-icon.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
